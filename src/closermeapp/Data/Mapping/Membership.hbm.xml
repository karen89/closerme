<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="closermeapp.Bussiness.Entities.Membership" table="membership">
        <id name="membershipNumber" column="membershipNumber">
            <generator class="identity"/>

        </id>
        <property name="membershipType" type="java.lang.String" column="membershipType"/>
        <property name="startDate"/>
        <property name="expireDate"/>
        <property name="costs"/>
    </class>
</hibernate-mapping>